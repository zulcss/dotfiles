#
# Container aliases
# 
elinks() {
    docker run --rm -it \
    --name elinks elinks "$@"
}

ab() {
    docker run --net host --rm -i -t \
        --name ab ab "$@"
}

aws() {
    docker run -it --rm \
        -v "${HOME}/.aws:/root/.aws" \
        aws "$@"
}

powershell() {
    docker run -it --rm powershell "$@"
}

nmap() {
    docker run -it --rm nmap "$@"
}

jq() {
    docker run -it --rm jq "@"
}
